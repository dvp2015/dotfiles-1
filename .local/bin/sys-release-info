#!/bin/bash

#
# Get information on Linux system
#
# dvp, Apr 2022
#

printf "\nOS Release:\n"
echo "-------------------"
cat /etc/os-release

printf  "\nDistribution:\n"
echo "-------------------"
cat /etc/upstream-release/lsb-release

printf "\nKernel:\n"
echo "-------------------"
uname -r



